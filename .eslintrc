{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "semi": [2, "never"],
    "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }],
    "react/forbid-prop-types": ["error", { forbid: [] }],
  },
  "globals": {
    "__DEV__": true,
    "it": true,
    "expect": true,
    "jest": true,
    "navigator": true,
  }
}
